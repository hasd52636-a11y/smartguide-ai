{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/proxy/gemini/chat",
      "dest": "/api/proxy/gemini/chat.js"
    },
    {
      "src": "/api/proxy/gemini/status",
      "dest": "/api/proxy/gemini/status.js"
    },
    {
      "src": "/api/proxy/zhipu/chat",
      "dest": "/api/proxy/zhipu/chat.js"
    },
    {
      "src": "/api/proxy/zhipu/status",
      "dest": "/api/proxy/zhipu/status.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}